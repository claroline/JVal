{
    "name": "stefk/jval",
    "description": "JSON Schema validator",
    "keywords": ["JSON", "JSON Schema", "validation"],
    "homepage": "https://github.com/stefk/JVal",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "StÃ©phane Klein",
            "email": "stephaneklein221@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "json-schema/json-schema": "dev-draft-6",
        "json-schema/test-suite": "dev-draft-6",
        "fabpot/php-cs-fixer": "~1.10"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "json-schema/json-schema",
                "version": "dev-draft-6",
                "source": {
                    "url": "https://github.com/json-schema-org/json-schema-spec",
                    "type": "git",
                    "reference": "3ad80f0f318f0d795f85b8b427a733cf5189e394"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "json-schema/test-suite",
                "version": "dev-draft-6",
                "source": {
                    "url": "https://github.com/json-schema-org/JSON-Schema-Test-Suite",
                    "type": "git",
                    "reference": "728066f9c5c258ba3b1804a22a5b998f2ec77ec0"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": { "JVal\\": "src/" }
    },
    "bin": ["bin/jval"],
    "minimum-stability": "dev",
    "prefer-stable": true
}
